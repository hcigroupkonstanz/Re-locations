{
  "name": "colibri-server",
  "version": "1.0.0",
  "main": "dist/server/main.js",
  "scripts": {
    "gui-build": "ng build --configuration production",
    "gui-watch": "ng build --watch",
    "server-watch-ts": "tsc --p src/server -w",
    "server-watch-node": "nodemon --watch dist/server/* --exec \"npm run server-start\"",
    "server-build": "tsc --p src/server",
    "server-start": "node dist/server/main.js",
    "debug": "concurrently -k -p \"[{name}]\" -n \"Angular,Node\" -c \"green.bold,blue.bold\" \"npm run gui-watch\" \"npm run server-watch-ts\"",
    "build": "npm run gui-build && npm run server-build",
    "start": "npm run server-start",
    "watch": "concurrently -k -p \"[{name}]\" -n \"Angular,Server-Compile,Server-Node\" -c \"green.bold,blue.bold,yellow.bold\" \"npm run gui-watch\" \"npm run server-watch-ts\" \"npm run server-watch-node\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.3.12",
    "@angular/cdk": "^13.3.9",
    "@angular/common": "~13.3.12",
    "@angular/compiler": "~13.3.12",
    "@angular/core": "~13.3.12",
    "@angular/forms": "~13.3.12",
    "@angular/material": "^13.3.9",
    "@angular/platform-browser": "~13.3.12",
    "@angular/platform-browser-dynamic": "~13.3.12",
    "@angular/router": "~13.3.12",
    "body-parser": "^1.19.0",
    "core-js": "^3.6.4",
    "cors": "^2.8.5",
    "csv-parse": "^4.8.8",
    "express": "^4.17.1",
    "lodash": "^4.17.15",
    "node-abi": "^2.15.0",
    "perfect-scrollbar": "^1.5.0",
    "rxjs": "~6.5.4",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "source-map-support": "^0.5.16",
    "tedious": "^8.0.1",
    "wavefile": "^11.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.10",
    "@angular/cli": "~13.3.10",
    "@angular/compiler-cli": "~13.3.12",
    "@angular/language-service": "~13.3.12",
    "@types/body-parser": "^1.19.0",
    "@types/csv-parse": "^1.2.2",
    "@types/express": "^4.17.3",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.8",
    "@types/lodash": "^4.14.149",
    "@types/moment": "^2.13.0",
    "@types/node": "^13.9.1",
    "@types/perfect-scrollbar": "^1.3.0",
    "@types/socket.io": "^2.1.4",
    "@types/socket.io-client": "^1.4.32",
    "@types/tedious": "^4.0.0",
    "codelyzer": "^6.0.0",
    "concurrently": "^5.1.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "nodemon": "^2.0.2",
    "protractor": "~7.0.0",
    "ts-node": "~8.6.2",
    "tslint": "~6.1.0",
    "typescript": "4.6.4"
  }
}
